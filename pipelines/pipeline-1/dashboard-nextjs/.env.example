# Frontend Environment Variables
# Copy this file to .env.local and update with your values

# ClickHouse Configuration (server-side only, not exposed to client)
# Use port 8123 for HTTP interface
CLICKHOUSE_HOST=http://localhost:8123
CLICKHOUSE_USER=admin
CLICKHOUSE_PASSWORD=clickhouse08062013
CLICKHOUSE_DATABASE=analytics

# Kafka Configuration (for Spotify live stream)
# Use port 9093 for external connections (host machine)
# Use port 9092 for internal connections (Docker containers)
KAFKA_BOOTSTRAP_SERVERS=localhost:9093
KAFKA_TOPIC=spotify.player.current
